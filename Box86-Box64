# Install related kits :

sudo apt install git wget make cmake build-essential gcc-arm-linux-gnueabihf 

# Compile Box86

cd
git clone https://github.com/ptitSeb/box86
cd box86
mkdir build; cd build; cmake .. -DARM64=1 -DBAD_SIGNAL=ON -DCMAKE_BUILD_TYPE=RelWithDebInfo
# mkdir build; cd build; cmake .. -DARM_DYNAREC=ON -DBAD_SIGNAL=ON -DCMAKE_BUILD_TYPE=RelWithDebInfo
make -j$(nproc)
sudo make install
cd
rm -rf box86

# Compile Box64

cd
git clone https://github.com/ptitSeb/box64
cd box64
mkdir build; cd build; cmake .. -DARM_DYNAREC=ON -DBAD_SIGNAL=ON -DCMAKE_BUILD_TYPE=RelWithDebInfo
make -j$(nproc)
sudo make install
cd
rm -rf box64



