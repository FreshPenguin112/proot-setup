# Install related kits :

sudo apt install git make cmake gcc-arm-linux-gnueabihf gcc-arm-linux-gnueabi build-essential

# Compile Box86

cd
git clone https://github.com/ptitSeb/box86
cd box86
mkdir build; cd build; cmake .. -DARM64=1 -DBAD_SIGNAL=1 -DCMAKE_BUILD_TYPE=RelWithDebInfo
make -j4
sudo make install
cd
rm -rf box86

# Compile Box64

cd
git clone https://github.com/ptitSeb/box64
cd box64
mkdir build; cd build; cmake .. -DARM_DYNAREC=1 -DBAD_SIGNAL=1 -DCMAKE_BUILD_TYPE=RelWithDebInfo
make -j4
sudo make install
cd
rm -rf box64



