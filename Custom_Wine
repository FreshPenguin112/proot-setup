# Wine-i386

cd 

mkdir wine

cd wine 

wget https://github.com/Kron4ek/Wine-Builds/releases/download/7.0/wine-7.0-x86.tar.xz

tar -xvf *.tar.xz

mv ~/wine/wine*/* ~/wine

rm -r wine*

cd

# Wine-amd64

cd 

mkdir wine64

cd wine64

wget https://github.com/Kron4ek/Wine-Builds/releases/download/7.0/wine-7.0-amd64.tar.xz

tar -xvf *.tar.xz

mv ~/wine64/wine*/* ~/wine64

rm -r wine*

cd



